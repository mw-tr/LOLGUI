-- Scripts:

local function FEOFMO_script() -- ExecuteButton.LocalScript 
	local script = Instance.new('LocalScript', ExecuteButton)

	local DEBOUNCE_TIME = 1 
	local debounce = false
	local lastRemoteEvent = nil
	
	local function getRemoteEvent()
		if lastRemoteEvent and lastRemoteEvent.Parent then
			return lastRemoteEvent
		end
	
		for _, child in ipairs(game:GetService('SoundService'):GetDescendants()) do
			if child:IsA("RemoteEvent") then
				lastRemoteEvent = child
				return child
			end
		end
		return nil
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if debounce then return end
		debounce = true
	
		local remoteEvent = getRemoteEvent()
	
		if not remoteEvent then
			debounce = false
			return
		end
	
		local loadstring = script.Parent.Parent.Text
		remoteEvent:FireServer(loadstring)
	
		task.delay(DEBOUNCE_TIME, function()
			debounce = false
		end)
	end)
	
	task.spawn(function()
		while true do
			lastRemoteEvent = getRemoteEvent()
			task.wait(0.1)  
		end
	end)
end
coroutine.wrap(FEOFMO_script)()
local function ARDKJF_script() -- ClearButton.ClearTextBox 
	local script = Instance.new('LocalScript', ClearButton)

	local button = script.Parent
	local textBox = button.Parent
	
	button.Activated:Connect(function()
	    textBox.Text = ""
	end)
	
	
end
coroutine.wrap(ARDKJF_script)()
local function YMAA_script() -- MainFrame.UIDrag 
	local script = Instance.new('LocalScript', MainFrame)

	-- Made by Real_IceyDev (@lceyDex) --
	-- Simple UI dragger (PC Only/Any device that has a mouse) --
	
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(YMAA_script)()
local function BLAZQZ_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	local TweenService = game:GetService("TweenService")
	
	local OptionFrame = script.Parent.Parent.Folder.OptionFrame
	local originalPosition = UDim2.new(-0.244, -4,0.009, -3) 
	local targetPosition = UDim2.new(0.009, -4, 0.009, -3) 
	
	local tweenInfo = TweenInfo.new(
		0.5, 
		Enum.EasingStyle.Circular, 
		Enum.EasingDirection.InOut 
	)
	
	local toTargetTween = TweenService:Create(OptionFrame, tweenInfo, {Position = targetPosition})
	local toOriginalTween = TweenService:Create(OptionFrame, tweenInfo, {Position = originalPosition})
	
	local ImageButton = script.Parent
	local isAtTarget = false
	
	ImageButton.MouseButton1Click:Connect(function()
		if isAtTarget then
			toOriginalTween:Play()
		else
			toTargetTween:Play() 
		end
		isAtTarget = not isAtTarget 
	end)
	
end
coroutine.wrap(BLAZQZ_script)()
